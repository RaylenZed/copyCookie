{
    "manifest_version": 3,
    "name": "Cookie 获取器",
    "version": "1.0",
    "description": "获取当前页面的所有 cookie，包括 HttpOnly",
    "permissions": [
      "activeTab",
      "cookies",
      "tabs",
      "scripting",
      "debugger"
    ],
    "host_permissions": [
      "<all_urls>"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }